{
    "name": "mean-course",
    "version": "0.1.0",
    "description": "mean-poc",
    "keywords": [],
    "homepage": "https://github.com/krbaio3/mean-poc#readme",
    "bugs": {
        "url": "https://github.com/krbaio3/mean-poc/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/krbaio3/mean-poc.git"
    },
    "license": "MIT",
    "author": "Jorge KrBaIO3 krbaio3@gmail.com",
    "main": "index.js",
    "scripts": {
        "debug": "nodemon --nolazy  --inspect-brk --require @babel-node --inspect=42894 --debug-brk inspect lib/index.js --exec babel-node",
        "build2": "npm run build:dev",
        "watch:ts": "tsc -w",
        "watch:node": "nodemon dist/index.js",
        "start": "concurrently -k -p \"[{name}]\" -n \"TypeScript, Node\" -c \"yello.bold, cyan.bold\" \"yarn run watch:ts\" \"yarn run watch:node\"",
        "build": "tsc",
        "dev": "ts-node ./lib/server.ts",
        "prod": "npm run build && npm run start",
        "start-old": "nodemon ./lib/index.js --exec babel-node",
        "build3": "babel lib --out-dir dist",
        "serve2": "node dist/server.js",
        "mongod:stop": "sudo service mongod stop",
        "mongod:start": "sudo service mongod start",
        "mongod:restart": "sudo service mongod restart",
        "mongod:logs": "cat /var/log/mongodb/mongod.log",
        "mongod:mongo": "mongo --host 127.0.0.1:27017",
        "mongod": "yarn mongod:start && yarn mongod:mongo",
        "build:aot": "npm run build:aot:prod",
        "build:aot:prod": "npm run clean:dist && npm run clean:aot && cross-env BUILD_AOT=1 npm run webpack -- --config config/webpack.prod.js  --progress --profile --bail",
        "build:dev": "npm run clean:dist && npm run webpack -- --config config/webpack.dev.js --progress --profile",
        "build:docker": "npm run build:prod && docker build -t angular2-webpack-start:latest .",
        "build:prod": "npm run clean:dist && npm run webpack -- --config config/webpack.prod.js  --progress --profile --bail",
        "ci": "npm run ci:testall",
        "ci:aot": "npm run lint && npm run test && npm run build:aot && npm run e2e",
        "ci:jit": "npm run lint && npm run test && npm run build:prod && npm run e2e",
        "ci:nobuild": "npm run lint && npm test && npm run e2e",
        "ci:testall": "npm run lint && npm run test && npm run build:prod && npm run e2e && npm run build:aot && npm run e2e",
        "ci:travis": "npm run lint && npm run test && npm run build:aot && npm run e2e:travis",
        "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist compiled dll",
        "clean:aot": "npm run rimraf -- compiled",
        "clean:dist": "npm run rimraf -- dist",
        "clean:dll": "npm run rimraf -- dll",
        "preclean:install": "npm run clean",
        "clean:install": "npm set progress=false && npm install",
        "docker": "docker",
        "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
        "e2e": "npm-run-all -p -r server:prod:ci protractor",
        "e2e:live": "npm-run-all -p -r server:prod:ci protractor:live",
        "e2e:travis": "npm-run-all -p -r server:prod:ci protractor:delay",
        "github-deploy": "npm run github-deploy:dev",
        "github-deploy:dev": "npm run webpack -- --config config/webpack.github-deploy.js --progress --profile --env.githubDev",
        "github-deploy:prod": "npm run webpack -- --config config/webpack.github-deploy.js --progress --profile --env.githubProd",
        "postinstall": "npm run webdriver:update",
        "lint": "npm run tslint \"src/**/*.ts\"",
        "node": "node",
        "protractor": "protractor",
        "protractor:delay": "sleep 3 && npm run protractor",
        "protractor:live": "protractor --elementExplorer",
        "rimraf": "rimraf",
        "server": "npm run server:dev",
        "server:dev": "npm run webpack-dev-server -- --config config/webpack.dev.js --open --progress --profile --watch --content-base src/",
        "server:dev:hmr": "npm run server:dev -- --inline --hot",
        "server:prod": "http-server dist -c-1 --cors",
        "server:prod:ci": "http-server dist -p 3000 -c-1 --cors",
        "start2": "concurrently \"npm run server:dev\" \"nodemon --watch server server-start.js\" ",
        "start:hmr": "npm run server:dev:hmr",
        "start:prod": "NODE_ENV=production node server-start.js",
        "test": "npm run lint && karma start",
        "tslint": "tslint",
        "typedoc": "typedoc",
        "preversion": "npm test",
        "version": "npm run build",
        "postversion": "git push && git push --tags",
        "watch": "npm run watch:dev",
        "watch:dev": "npm run build:dev -- --watch",
        "watch:dev:hmr": "npm run watch:dev -- --hot",
        "watch:prod": "npm run build:prod -- --watch",
        "watch:test": "npm run test -- --auto-watch --no-single-run",
        "webdriver-manager": "webdriver-manager",
        "webdriver:start": "npm run webdriver-manager start",
        "webdriver:update": "webdriver-manager update",
        "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js",
        "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js"
    },
    "dependencies": {
        "apollo-angular": "^1.4.1",
        "apollo-client": "^2.4.2",
        "app-root-path": "^2.1.0",
        "bcrypt-nodejs": "^0.0.3",
        "bluebird": "^3.5.2",
        "body-parser": "^1.18.3",
        "casual": "^1.5.19",
        "connect-multiparty": "^2.2.0",
        "cookie-parser": "^1.4.3",
        "core-js": "^2.5.7",
        "cors": "^2.8.4",
        "debug": "^4.1.0",
        "dotenv": "^6.1.0",
        "express": "^4.16.3",
        "express-graphql": "^0.6.12",
        "express-jwt": "^5.3.1",
        "express-validation": "^1.0.2",
        "express-winston": "^3.0.0",
        "graphql": "^14.0.2",
        "graphql-server-express": "latest",
        "graphql-tag": "^2.10.0",
        "graphql-tools": "^4.0.0",
        "hapi": "^17.6.0",
        "helmet": "^3.14.0",
        "http-errors": "^1.7.1",
        "http-server": "^0.11.1",
        "http-status": "^1.2.0",
        "joi": "^14.0.0",
        "jwt-simple": "^0.5.5",
        "method-override": "^3.0.0",
        "moment": "^2.22.2",
        "mongoose": "^5.3.6",
        "mongoose-pagination": "^1.0.0",
        "morgan": "^1.9.1",
        "passport": "^0.4.0",
        "preboot": "^6.0.0-beta.6",
        "winston": "^3.1.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.0.0",
        "@babel/core": "^7.0.0",
        "@babel/node": "^7.0.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
        "@babel/plugin-syntax-async-generators": "^7.0.0",
        "@babel/plugin-transform-destructuring": "^7.0.0",
        "@babel/polyfill": "^7.0.0",
        "@babel/preset-env": "^7.0.0",
        "@babel/preset-typescript": "^7.1.0",
        "@babel/register": "^7.0.0",
        "@types/bluebird": "^3.5.24",
        "@types/bluebird-global": "^3.5.9",
        "@types/body-parser": "1.17.0",
        "@types/chai": "^4.1.6",
        "@types/cookie-parser": "^1.3.30",
        "@types/errorhandler": "0.0.32",
        "@types/express": "^4.16.0",
        "@types/method-override": "0.0.31",
        "@types/mocha": "^5.2.5",
        "@types/mongodb": "^3.1.12",
        "@types/mongoose": "^5.2.19",
        "@types/mongoose-promise": "^4.5.7",
        "@types/morgan": "^1.7.32",
        "@types/node": "^10.11.6",
        "@types/passport": "^0.4.6",
        "concurrently": "^4.0.1",
        "cross-env": "^5.2.0",
        "find-root": "^1.1.0",
        "gh-pages": "^2.0.1",
        "json-loader": "^0.5.7",
        "nodemon": "^1.18.4",
        "npm-run-all": "^4.1.3",
        "optimize-js-plugin": "0.0.4",
        "parse5": "^5.1.0",
        "rimraf": "~2.6.2",
        "ts-loader": "^5.2.2",
        "ts-node": "^7.0.1",
        "tslib": "^1.9.3",
        "tslint": "~5.11.0",
        "tslint-loader": "^3.5.4",
        "typedoc": "^0.13.0",
        "typescript": "~3.1.1"
    },
    "engines": {
        "node": ">=8.0.0",
        "npm": ">=5.0.0"
    }
}